<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/primaryContainer" /> <!-- Remplacez "votre_couleur_de_fond" par la couleur de fond souhaitée -->
            <corners android:radius="64dp" />
            <stroke
                android:width="2dp"
                android:color="@color/onPrimaryContainer" /> <!-- Remplacez "onPrimaryContainer" par la couleur de bordure souhaitée -->
        </shape>
    </item>
</layer-list>
